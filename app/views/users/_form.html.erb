<%= form_for :user, url: '/users' do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :Password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.label :location %>
<%= f.text_field :location %>
<%= f.label :email %>
<%= f.text_field :email %>

 <%= f.submit "Create User" %>

<% end %>
